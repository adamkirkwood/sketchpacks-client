{
  "name": "sketchpacks-app",
  "productName": "Sketchpacks",
  "companyName": "Sketchpacks",
  "description": "Discover and manage Sketch Plugins from the menu bar",
  "version": "0.2.1",
  "main": "index.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "client:electron": "NODE_ENV=development electron .",
    "serve:electron": "NODE_ENV=development webpack-dev-server --config webpack.config.electron.dev.js",
    "serve:web": "NODE_ENV=development webpack-dev-server --target web --config webpack.config.web.dev.js",
    "build:web": "NODE_ENV=production webpack -p --target web --config webpack.config.web.prod.js",
    "deploy:web": "npm run build:web && firebase deploy",
    "build": "NODE_ENV=production webpack -p --progress --profile --colors --config webpack.config.electron.prod.js",
    "pack": "npm run build && build --dir",
    "pack:dev": "npm run build && build --dir",
    "dist": "npm run build && build"
  },
  "author": "Adam Kirkwood <hello@adamkirkwood.com>",
  "license": "ISC",
  "build": {
    "appId": "DGHF878J4A.com.sketchpacks.macos",
    "asar": false,
    "protocols": {
      "name": "Sketchpacks",
      "schemes": [
        "sketchpacks"
      ]
    },
    "mac": {
      "category": "public.app-category.productivity",
      "icon": "build/icon.icns"
    }
  },
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-inline-environment-variables": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-native-modules": "^6.9.4",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.20.0",
    "css-loader": "^0.26.1",
    "electron-builder": "^10.6.1",
    "electron-packager": "^8.4.0",
    "electron-prebuilt": "^1.4.11",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "html-webpack-plugin": "^2.24.1",
    "node-sass": "^4.1.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.0-rc.1",
    "webpack-dev-server": "^2.1.0-beta.12",
    "webpack-target-electron-renderer": "^0.3.0"
  },
  "dependencies": {
    "electron-log": "^1.2.2",
    "express": "^4.14.0",
    "fs": "0.0.1-security",
    "is-electron-renderer": "^2.0.0",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.2",
    "marky-markdown": "^9.0.1",
    "menubar": "^5.1.0",
    "minimist": "^1.2.0",
    "ms": "^0.7.2",
    "node-notifier": "^4.6.1",
    "node-sass": "^4.0.0",
    "os": "^0.1.1",
    "parse-link-header": "^0.4.1",
    "qs": "^6.3.0",
    "react": "^15.4.1",
    "react-addons-create-fragment": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-pagify": "^2.1.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "segmentize": "^0.4.1",
    "whatwg-fetch": "^2.0.1"
  }
}
