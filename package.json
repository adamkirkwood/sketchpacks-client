{
  "name": "sketchpacks-app",
  "productName": "Sketchpacks",
  "companyName": "Sketchpacks",
  "description": "Discover and manage Sketch Plugins from the menu bar",
  "version": "0.4.1",
  "main": "index.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "client:electron": "NODE_ENV=development electron .",
    "serve:electron": "NODE_ENV=development webpack-dev-server --config webpack.config.electron.dev.js",
    "serve:web": "NODE_ENV=development webpack-dev-server --target web --config webpack.config.web.dev.js",
    "build:web": "NODE_ENV=production webpack -p --target web --config webpack.config.web.prod.js",
    "deploy:web": "npm run build:web && firebase deploy",
    "build": "NODE_ENV=production webpack -p --progress --profile --colors --config webpack.config.electron.prod.js",
    "pack": "npm run build && build --dir",
    "pack:dev": "npm run build && build --dir",
    "dist": "npm run build && build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jestsetup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/src/__mocks__/styleMock.js"
    }
  },
  "author": "Adam Kirkwood <hello@adamkirkwood.com>",
  "license": "ISC",
  "build": {
    "appId": "DGHF878J4A.com.sketchpacks.macos",
    "asar": true,
    "asarUnpack": "node_modules/node-notifier/vendor/**",
    "prune": true,
    "protocols": {
      "name": "Sketchpacks",
      "schemes": [
        "sketchpacks"
      ]
    },
    "mac": {
      "category": "public.app-category.productivity",
      "icon": "build/icon.icns"
    }
  },
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-inline-environment-variables": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-native-modules": "^6.9.4",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.20.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "electron-builder": "^10.6.1",
    "electron-packager": "^8.4.0",
    "electron-prebuilt": "^1.4.11",
    "enzyme": "^2.7.1",
    "enzyme-to-json": "^1.5.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.24.1",
    "jest": "^19.0.2",
    "node-sass": "^4.1.1",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0-rc.1",
    "webpack-dev-server": "^2.1.0-beta.12",
    "webpack-target-electron-renderer": "^0.3.0"
  },
  "dependencies": {
    "adm-zip": "^0.4.7",
    "async": "^2.1.4",
    "axios": "^0.15.3",
    "content-disposition": "^0.5.2",
    "dblite": "^0.7.8",
    "electron-log": "^1.2.2",
    "first-run": "^1.2.0",
    "fs": "0.0.1-security",
    "immutability-helper": "^2.1.2",
    "is-electron-renderer": "^2.0.0",
    "is-there": "^4.3.6",
    "jsonfile": "^2.4.0",
    "lodash": "^4.17.2",
    "marky-markdown": "^9.0.1",
    "menubar": "^5.1.0",
    "minimist": "^1.2.0",
    "moment": "^2.17.1",
    "ms": "^0.7.2",
    "nedb": "^1.8.0",
    "node-notifier": "^4.6.1",
    "node-sass": "^4.0.0",
    "normalize.css": "^5.0.0",
    "os": "^0.1.1",
    "parse-link-header": "^0.4.1",
    "promise": "^7.1.1",
    "qs": "^6.3.0",
    "react": "^15.4.1",
    "react-addons-create-fragment": "^15.4.1",
    "react-addons-shallow-compare": "^15.4.2",
    "react-dom": "^15.4.1",
    "react-lazyload": "^2.2.4",
    "react-markdown": "^2.4.3",
    "react-redux": "^5.0.0-rc.2",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-waypoint": "^5.1.0",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.2.0",
    "remimi": "^1.1.0",
    "request": "^2.79.0",
    "reselect": "^2.5.4",
    "respinner": "^1.2.1",
    "semver": "^5.3.0",
    "url": "^0.11.0"
  }
}
